{
	"name": "LOAD_IMPROVE_L0",
	"properties": {
		"activities": [
			{
				"name": "IterateSqlTables",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"activities": [
						{
							"name": "LOOKUP_OLD_LOAD",
							"type": "Lookup",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"sqlReaderQuery": {
										"value": "select MAX(@{item().Rif_Column}) as OldRifValue from @{item().TABLE_NAME}",
										"type": "Expression"
									}
								},
								"dataset": {
									"referenceName": "CSP_INCREMENTAL_LOAD",
									"type": "DatasetReference"
								}
							}
						},
						{
							"name": "LOOKUP_NEW_LOAD",
							"type": "Lookup",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"sqlReaderQuery": {
										"value": "select MAX(@{item().Rif_Column}) as NewRifValue from @{item().TABLE_NAME}",
										"type": "Expression"
									}
								},
								"dataset": {
									"referenceName": "DS_L0_TABLE_IMPROVE",
									"type": "DatasetReference"
								}
							}
						},
						{
							"name": "INCREMENTAL_COPY_ACTIVITY",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "LOOKUP_OLD_LOAD",
									"dependencyConditions": [
										"Succeeded"
									]
								},
								{
									"activity": "LOOKUP_NEW_LOAD",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"sqlReaderQuery": {
										"value": "select * from @{item().TABLE_NAME} where @{item().Rif_Column} > '@{activity('LOOKUP_OLD_LOAD').output.firstRow.OldRifValue}' and @{item().WaterMark_Column} <= '@{activity('LOOKUP_NEW_LOAD').output.firstRow.NewRifValue}'",
										"type": "Expression"
									}
								},
								"sink": {
									"type": "AzureSqlSink",
									"sqlWriterStoredProcedureName": {
										"value": "@{item().StoredProcedureNameForMergeOperation}",
										"type": "Expression"
									},
									"sqlWriterTableType": {
										"value": "@{item().TableType}",
										"type": "Expression"
									},
									"storedProcedureTableTypeParameterName": {
										"value": "@{item().TABLE_NAME}",
										"type": "Expression"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "DS_L0_TABLE_IMPROVE",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "CSP_EDWH_DATA_SET",
									"type": "DatasetReference",
									"parameters": {
										"ImproveTableName": "@{item().TABLE_NAME}"
									}
								}
							]
						},
						{
							"name": "UpdateRifValue",
							"type": "SqlServerStoredProcedure",
							"dependsOn": [
								{
									"activity": "INCREMENTAL_COPY_ACTIVITY",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"storedProcedureName": "dbo.usp_write_rif_value"
							},
							"linkedServiceName": {
								"referenceName": "CSP_EDWH_LINKED_SERVER",
								"type": "LinkedServiceReference"
							}
						}
					]
				}
			}
		],
		"parameters": {
			"tablesList": {
				"type": "object"
			}
		},
		"annotations": []
	}
}