{
	"name": "SET_TENANTS",
	"properties": {
		"activities": [
			{
				"name": "COPY_L0_TENANTS",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "select * from dbo.AbpTenants",
							"type": "Expression"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "delete from dbo.L0_IMPR_AbpTenants",
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"parallelCopies": 1
				},
				"inputs": [
					{
						"referenceName": "DS_L0_TABLE_IMPROVE",
						"type": "DatasetReference",
						"parameters": {
							"serverName": {
								"value": "@pipeline().parameters.IMPRServerName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "EDWH_LOAD_L0",
						"type": "DatasetReference",
						"parameters": {
							"DN_TARGET": {
								"value": "L0_IMPR_AbpTenants",
								"type": "Expression"
							},
							"serverName": {
								"value": "@pipeline().parameters.EDWHServerName",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "MERGE MD TENANTS",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "COPY_L0_TENANTS",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "dbo.MD_LOAD_TENANTS"
				},
				"linkedServiceName": {
					"referenceName": "CSP_EDWH_LINKED_SERVER",
					"type": "LinkedServiceReference",
					"parameters": {
						"ServerName": {
							"value": "@pipeline().parameters.EDWHServerName",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"EXEC_ENV": {
				"type": "string",
				"defaultValue": "svil"
			},
			"IMPRServerName": {
				"type": "string",
				"defaultValue": "tcp:improve-beta-sql-srv.database.windows.net, 1433"
			},
			"EDWHServerName": {
				"type": "string",
				"defaultValue": "tcp:edwh-svil-sql-srv.database.windows.net,1433"
			}
		},
		"annotations": []
	}
}