													"value": null,
													"type": "String"
												}
											}
										},
										"linkedServiceName": {
											"referenceName": "CSP_EDWH_LINKED_SERVER",
											"type": "LinkedServiceReference",
											"parameters": {
												"ServerName": {
													"value": "@variables('EDWHServerName')",
													"type": "Expression"
												}
											}
										}
									},
