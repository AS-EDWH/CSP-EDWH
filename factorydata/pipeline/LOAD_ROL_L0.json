											"type": "LinkedServiceReference"
										}
									}
								]
							}
						}
					]
				}
			},
			{
				"name": "GetExecParam",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderStoredProcedureName": "[dbo].[MD_LOAD_L0_PARAMS]",
						"storedProcedureParameters": {
							"DN_SOURCE": {
								"type": "String",
								"value": {
									"value": "@pipeline().parameters.DN_SOURCE",
									"type": "Expression"
								}
							}
						}
					},
					"dataset": {
						"referenceName": "CSP_INCREMENTAL_LOAD",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			}
		],
		"parameters": {
			"DN_SOURCE": {
				"type": "string",
				"defaultValue": "ROL"
			},
			"EXEC_ENV": {
				"type": "string",
				"defaultValue": "svil"
			}
		},
		"annotations": []
	}
