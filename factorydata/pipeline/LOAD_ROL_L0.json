											},
											"sink": {
												"type": "AzureSqlSink",
												"preCopyScript": {
													"value": "delete from @{item().DN_TARGET}",
													"type": "Expression"
												},
												"tableOption": "autoCreate",
												"disableMetricsCollection": false
											},
											"enableStaging": false,
											"parallelCopies": 1
										},
										"inputs": [
											{
												"referenceName": "DS_L0_TABLE_ROL",
												"type": "DatasetReference",
