{
	"name": "LOAD_ROL_L0",
	"properties": {
		"activities": [
			{
				"name": "Loop_Load_Tables",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "GetExecParam",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('GetExecParam').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "SET_START",
							"description": "Inserisce un'occorrenza sulla tabella di LOG impostando DT_START con getdate()",
							"type": "Lookup",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"sqlReaderStoredProcedureName": "[dbo].[MD_ERROR_SET_START]",
									"storedProcedureParameters": {
										"pDN_STEP": {
											"type": "String",
											"value": {
												"value": "@concat('LOAD_' ,variables('DN_SOURCE'),'_',item().DN_TABLE_NAME)",
												"type": "Expression"
											}
										},
										"pDN_STEP_DETAIL": {
											"type": "String",
											"value": {
												"value": "@item().DN_TARGET",
												"type": "Expression"
											}
										},
										"pDN_SOURCE": {
											"type": "String",
											"value": null
										}
									}
								},
								"dataset": {
									"referenceName": "CSP_INCREMENTAL_LOAD",
									"type": "DatasetReference",
									"parameters": {
										"serverName": {
											"value": "@variables('EDWHServerName')",
											"type": "Expression"
										}
									}
								}
							}
