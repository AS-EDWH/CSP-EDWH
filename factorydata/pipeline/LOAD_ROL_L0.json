											"secureOutput": false,
											"secureInput": false
										},
										"userProperties": [],
										"typeProperties": {
											"storedProcedureName": "[dbo].[MD_ERROR_END_KO]",
											"storedProcedureParameters": {
												"pID_RUN": {
													"value": {
														"value": "@activity('SET_START').output.FirstRow.ID_RUN",
														"type": "Expression"
													},
													"type": "int"
												},
												"pErrorMessage": {
													"value": {
														"value": "@activity('LOAD_ROL_ALL_TARGET_TABLES').output.errors[0].message",
