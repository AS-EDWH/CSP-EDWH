							"type": "IfCondition",
							"dependsOn": [
								{
									"activity": "SET_START",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"expression": {
									"value": "@equals(item().DN_RIF_COLUMN,'ALL')",
									"type": "Expression"
								},
								"ifFalseActivities": [
									{
										"name": "LOAD_ROL_TARGET_TABLES",
										"type": "Copy",
