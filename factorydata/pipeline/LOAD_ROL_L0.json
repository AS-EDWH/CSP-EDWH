			},
			{
				"name": "GetExecParam",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "Set EDWH Server Name",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "Set ROL Server Name",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderStoredProcedureName": "[dbo].[MD_LOAD_L0_PARAMS]",
						"storedProcedureParameters": {
							"DN_SOURCE": {
								"type": "String",
								"value": {
									"value": "@variables('DN_SOURCE')",
									"type": "Expression"
								}
							}
						}
					},
					"dataset": {
						"referenceName": "CSP_INCREMENTAL_LOAD",
						"type": "DatasetReference",
						"parameters": {
							"serverName": {
								"value": "@variables('EDWHServerName')",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Set EDWH Server Name",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "EDWHServerName",
					"value": {
						"value": "@if(equals(pipeline().parameters.EXEC_ENV,'svil'),variables('EDWH_SVIL_Server_Name'),variables('EDWH_PROD_Server_Name'))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set ROL Server Name",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "ROLServerName",
					"value": {
						"value": "@if(equals(pipeline().parameters.EXEC_ENV,'svil'),variables('ROL_SVIL_Server_Name'),variables('ROL_PROD_Server_Name'))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "EXEC LOAD L1",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Loop_Load_Tables",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "LOAD_L1",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"EXEC_ENV": {
							"value": "@pipeline().parameters.EXEC_ENV",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"EXEC_ENV": {
				"type": "string",
				"defaultValue": "svil"
			}
		},
		"variables": {
			"DN_SOURCE": {
				"type": "String",
				"defaultValue": "ROL"
			},
			"ROLServerName": {
				"type": "String"
			},
			"EDWH_PROD_Server_Name": {
				"type": "String",
				"defaultValue": "tcp:edwh-prod-sql-srv.database.windows.net,1433"
			},
			"EDWH_SVIL_Server_Name": {
				"type": "String",
				"defaultValue": "tcp:edwh-svil-sql-srv.database.windows.net,1433"
			},
			"ROL_PROD_Server_Name": {
				"type": "String",
				"defaultValue": "192.168.15.16"
			},
			"ROL_SVIL_Server_Name": {
				"type": "String",
				"defaultValue": "STRSRV-SQLALFAB\\ALFA2016"
			},
			"EDWHServerName": {
				"type": "String"
			}
		},
		"annotations": []
	}
