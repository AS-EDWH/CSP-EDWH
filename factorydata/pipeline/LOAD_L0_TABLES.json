{
	"name": "LOAD_L0_TABLES",
	"properties": {
		"activities": [
			{
				"name": "LOOKUP_OLD_LOAD",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderStoredProcedureName": "[dbo].[GET_RIF_COLUMN]",
						"storedProcedureParameters": {
							"TableName": {
								"type": "String",
								"value": {
									"value": "'@pipeline().parameters.DN_TABLE_NAME'",
									"type": "Expression"
								}
							}
						}
					},
					"dataset": {
						"referenceName": "CSP_INCREMENTAL_LOAD",
						"type": "DatasetReference"
					}
				}
			},
			{
				"name": "INCREMENTAL_COPY_ACTIVITY",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "LOOKUP_OLD_LOAD",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": " \n select * from @{pipeline().parameters.TABLE_NAME} where @{pipeline().parameters.Rif_Column} > \n '@{activity('LOOKUP_OLD_LOAD').output.firstRow.RIF_VALUE}'",
							"type": "Expression"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"sqlWriterStoredProcedureName": {
							"value": "@pipeline().parameters.DN_STORED_PROCEDURE",
							"type": "Expression"
						},
						"sqlWriterTableType": {
							"value": "@pipeline().parameters.DN_TABLE_TYPE",
							"type": "Expression"
						},
						"storedProcedureTableTypeParameterName": {
							"value": "@pipeline().parameters.DN_TABLE_NAME",
							"type": "Expression"
						},
						"disableMetricsCollection": false
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_L0_TABLE_IMPROVE",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CSP_EDWH_DATA_SET",
						"type": "DatasetReference",
						"parameters": {
							"ImproveTableName": {
								"value": "@pipeline().parameters.DN_TABLE_NAME",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"DN_TABLE_NAME": {
				"type": "string"
			},
			"DN_RIF_COLUMN": {
				"type": "string"
			},
			"DN_TABLE_TYPE": {
				"type": "string"
			},
			"DN_STORED_PROCEDURE": {
				"type": "string"
			}
		},
		"annotations": []
	}
}