{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CSP-EDWH"
		},
		"AS_SQL_SERVER_LIKED_SERVER_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AS_SQL_SERVER_LIKED_SERVER'"
		},
		"CSP_EDWH_LINKED_SERVER_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CSP_EDWH_LINKED_SERVER'"
		},
		"CSP_EDWH_PROD_LINKED_SERVER_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CSP_EDWH_PROD_LINKED_SERVER'"
		},
		"ImproveSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ImproveSource'"
		},
		"ImproveSourcePROD_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ImproveSourcePROD'"
		},
		"LS_CSPTO_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_CSPTO'"
		},
		"LS_CSPTO_PROD_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_CSPTO_PROD'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://EDWH-PASSWORD.vault.azure.net/"
		},
		"Sched_Load_IMPR_L0_properties_LOAD_IMPR_L0_parameters_EXEC_ENV": {
			"type": "string",
			"defaultValue": "prod"
		},
		"trigger1_properties_LOAD_ROL_L0_parameters_EXEC_ENV": {
			"type": "string",
			"defaultValue": "prod"
		},
		"trigger2_properties_EXEC_ETL_parameters_EXEC_ENV": {
			"type": "string",
			"defaultValue": "prod"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AS_SQL_SERVER_LIKED_SERVER_connectionString": {
						"value": "[parameters('AS_SQL_SERVER_LIKED_SERVER_connectionString')]"
					},
					"CSP_EDWH_LINKED_SERVER_connectionString": {
						"value": "[parameters('CSP_EDWH_LINKED_SERVER_connectionString')]"
					},
					"CSP_EDWH_PROD_LINKED_SERVER_connectionString": {
						"value": "[parameters('CSP_EDWH_PROD_LINKED_SERVER_connectionString')]"
					},
					"ImproveSource_connectionString": {
						"value": "[parameters('ImproveSource_connectionString')]"
					},
					"ImproveSourcePROD_connectionString": {
						"value": "[parameters('ImproveSourcePROD_connectionString')]"
					},
					"LS_CSPTO_connectionString": {
						"value": "[parameters('LS_CSPTO_connectionString')]"
					},
					"LS_CSPTO_PROD_connectionString": {
						"value": "[parameters('LS_CSPTO_PROD_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Sched_Load_IMPR_L0_properties_LOAD_IMPR_L0_parameters_EXEC_ENV": {
						"value": "[parameters('Sched_Load_IMPR_L0_properties_LOAD_IMPR_L0_parameters_EXEC_ENV')]"
					},
					"trigger1_properties_LOAD_ROL_L0_parameters_EXEC_ENV": {
						"value": "[parameters('trigger1_properties_LOAD_ROL_L0_parameters_EXEC_ENV')]"
					},
					"trigger2_properties_EXEC_ETL_parameters_EXEC_ENV": {
						"value": "[parameters('trigger2_properties_EXEC_ETL_parameters_EXEC_ENV')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}