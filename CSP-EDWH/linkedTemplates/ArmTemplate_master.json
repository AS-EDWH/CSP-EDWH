{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CSP-EDWH"
		},
		"AS_SQL_SERVER_LIKED_SERVER_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AS_SQL_SERVER_LIKED_SERVER'"
		},
		"AS_SQL_SERVER_LIKED_SERVER_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AS_SQL_SERVER_LIKED_SERVER'"
		},
		"CSP_EDWH_LINKED_SERVER_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CSP_EDWH_LINKED_SERVER'"
		},
		"AS_SQL_SERVER_LIKED_SERVER_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ITAdmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CSP_EDWH_LINKED_SERVER_connectionString": {
						"value": "[parameters('CSP_EDWH_LINKED_SERVER_connectionString')]"
					},
					"AS_SQL_SERVER_LIKED_SERVER_connectionString": {
						"value": "[parameters('AS_SQL_SERVER_LIKED_SERVER_connectionString')]"
					},
					"AS_SQL_SERVER_LIKED_SERVER_password": {
						"value": "[parameters('AS_SQL_SERVER_LIKED_SERVER_password')]"
					},
					"AS_SQL_SERVER_LIKED_SERVER_properties_typeProperties_userName": {
						"value": "[parameters('AS_SQL_SERVER_LIKED_SERVER_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}