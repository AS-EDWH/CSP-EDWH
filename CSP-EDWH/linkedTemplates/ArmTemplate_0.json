		{
			"name": "[concat(parameters('factoryName'), '/AS_SQL_SERVER_LIKED_SERVER')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('AS_SQL_SERVER_LIKED_SERVER_connectionString')]",
					"userName": "[parameters('AS_SQL_SERVER_LIKED_SERVER_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('AS_SQL_SERVER_LIKED_SERVER_password')]"
					}
				},
				"connectVia": {
					"referenceName": "AS-PC-IR",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/AS-PC-IR')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LS_CSPTO')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('LS_CSPTO_connectionString')]"
				},
				"connectVia": {
					"referenceName": "IR-SOURCE-CSPTO",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/IR-SOURCE-CSPTO')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LOAD_IMPR_L0')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Loop_Load_Tables",
						"type": "ForEach",
						"dependsOn": [
							{
								"activity": "GetExecParam",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@activity('GetExecParam').output.value",
								"type": "Expression"
							},
							"isSequential": true,
							"activities": [
								{
									"name": "SET_START",
									"type": "Lookup",
									"dependsOn": [],
									"policy": {
										"timeout": "7.00:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "AzureSqlSource",
											"sqlReaderStoredProcedureName": "[[dbo].[MD_ERROR_SET_START]",
											"storedProcedureParameters": {
												"pDN_STEP": {
													"type": "String",
													"value": {
														"value": "@concat('LOAD_' ,pipeline().parameters.DN_SOURCE,'_',item().DN_TABLE_NAME)",
														"type": "Expression"
													}
												},
												"pDN_STEP_DETAIL": {
													"type": "String",
													"value": {
														"value": "@item().DN_TARGET",
														"type": "Expression"
													}
												}
											}
										},
										"dataset": {
											"referenceName": "EDWH_DB",
											"type": "DatasetReference",
											"parameters": {}
										}
									}
								},
								{
									"name": "LOAD_IMPR_TARGET",
									"type": "IfCondition",
									"dependsOn": [
										{
											"activity": "SET_START",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"userProperties": [],
									"typeProperties": {
										"expression": {
											"value": "@equals(item().DN_RIF_COLUMN,'ALL')",
											"type": "Expression"
										},
										"ifFalseActivities": [
											{
												"name": "LOAD_IMPR_TARGET_TABLES",
												"type": "Copy",
												"dependsOn": [
													{
														"activity": "GET_REF_DATE",
														"dependencyConditions": [
															"Succeeded"
														]
													}
												],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"source": {
														"type": "SqlServerSource",
														"sqlReaderQuery": {
															"value": "select * from @{item().DN_TABLE_NAME} where @{item().DN_RIF_COLUMN} > \n '@{activity('GET_REF_DATE').output.firstRow.DT_START}'",
															"type": "Expression"
														}
													},
													"sink": {
														"type": "AzureSqlSink",
														"preCopyScript": {
															"value": "delete from @{item().DN_TARGET}",
															"type": "Expression"
														},
														"tableOption": "autoCreate",
														"disableMetricsCollection": false
													},
													"enableStaging": false,
													"parallelCopies": 1
												},
												"inputs": [
													{
														"referenceName": "DS_L0_TABLE_IMPROVE",
														"type": "DatasetReference",
														"parameters": {}
													}
												],
												"outputs": [
													{
														"referenceName": "EDWH_LOAD_L0",
														"type": "DatasetReference",
														"parameters": {
															"DN_TARGET": {
																"value": "@item().DN_TARGET",
																"type": "Expression"
															}
														}
													}
												]
											},
											{
												"name": "SET_END_OK",
												"type": "SqlServerStoredProcedure",
												"dependsOn": [
													{
														"activity": "LOAD_IMPR_TARGET_TABLES",
														"dependencyConditions": [
															"Succeeded"
														]
													}
												],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"storedProcedureName": "[[dbo].[MD_ERROR_END_OK]",
													"storedProcedureParameters": {
														"pID_RUN": {
															"value": {
																"value": "@activity('SET_START').output.FirstRow.ID_RUN",
																"type": "Expression"
															},
															"type": "int"
														},
														"pROWS_READ": {
															"value": {
																"value": "@activity('LOAD_IMPR_TARGET_TABLES').output.rowsRead",
																"type": "Expression"
															},
															"type": "int"
														},
														"pROWS_WRITTEN": {
															"value": {
																"value": "@activity('LOAD_IMPR_TARGET_TABLES').output.rowsCopied",
																"type": "Expression"
															},
															"type": "int"
														},
														"pCOPY_DURATION": {
															"value": {
																"value": "@activity('LOAD_IMPR_TARGET_TABLES').output.copyDuration",
																"type": "Expression"
															},
															"type": "int"
														},
														"pDATA_WRITTEN": {
															"value": {
																"value": "@activity('LOAD_IMPR_TARGET_TABLES').output.dataWritten",
																"type": "Expression"
															},
															"type": "Int64"
														},
														"pDESCR": {
															"value": null,
															"type": "String"
														}
													}
												},
												"linkedServiceName": {
													"referenceName": "CSP_EDWH_LINKED_SERVER",
													"type": "LinkedServiceReference"
												}
											},
											{
												"name": "SET_END_KO",
												"type": "SqlServerStoredProcedure",
												"dependsOn": [
													{
														"activity": "LOAD_IMPR_TARGET_TABLES",
														"dependencyConditions": [
															"Failed"
														]
													}
												],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"storedProcedureName": "[[dbo].[MD_ERROR_END_KO]",
													"storedProcedureParameters": {
														"pID_RUN": {
															"value": {
																"value": "@activity('SET_START').output.FirstRow.ID_RUN",
																"type": "Expression"
															},
															"type": "int"
														},
														"pErrorMessage": {
															"value": {
																"value": "@activity('LOAD_IMPR_TARGET_TABLES').output.errors[0].message",
																"type": "Expression"
															},
															"type": "string"
														}
													}
												},
												"linkedServiceName": {
													"referenceName": "CSP_EDWH_LINKED_SERVER",
													"type": "LinkedServiceReference"
												}
											},
											{
												"name": "GET_REF_DATE",
												"description": "Estrae dal DB la data di riferimento per l'estrazione del passo successivo",
												"type": "Lookup",
												"dependsOn": [],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"source": {
														"type": "AzureSqlSource",
														"sqlReaderStoredProcedureName": "[[dbo].[MD_GET_START_DATE]",
														"storedProcedureParameters": {
															"pDN_TABLE_NAME": {
																"type": "String",
																"value": {
																	"value": "@item().DN_TABLE_NAME",
																	"type": "Expression"
																}
															},
															"pDN_SOURCE": {
																"value": {
																	"value": "@pipeline().parameters.DN_SOURCE",
																	"type": "Expression"
																}
															}
														}
													},
													"dataset": {
														"referenceName": "EDWH_DB",
														"type": "DatasetReference",
														"parameters": {}
													}
												}
											}
										],
										"ifTrueActivities": [
											{
												"name": "LOAD_IMPR_ALL_TARGET_TABLES",
												"type": "Copy",
												"dependsOn": [],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"source": {
														"type": "SqlServerSource",
														"sqlReaderQuery": {
															"value": "select * from @{item().DN_TABLE_NAME} ",
															"type": "Expression"
														}
													},
													"sink": {
														"type": "AzureSqlSink",
														"preCopyScript": {
															"value": "delete from @{item().DN_TARGET}",
															"type": "Expression"
														},
														"tableOption": "autoCreate",
														"disableMetricsCollection": false
													},
													"enableStaging": false,
													"parallelCopies": 1
												},
												"inputs": [
													{
														"referenceName": "DS_L0_TABLE_IMPROVE",
														"type": "DatasetReference",
														"parameters": {}
													}
												],
												"outputs": [
													{
														"referenceName": "EDWH_LOAD_L0",
														"type": "DatasetReference",
														"parameters": {
															"DN_TARGET": {
																"value": "@item().DN_TARGET",
																"type": "Expression"
															}
														}
													}
												]
											},
											{
												"name": "SET_END_OK_ALL",
												"type": "SqlServerStoredProcedure",
												"dependsOn": [
													{
														"activity": "LOAD_IMPR_ALL_TARGET_TABLES",
														"dependencyConditions": [
															"Succeeded"
														]
													}
												],
												"policy": {
