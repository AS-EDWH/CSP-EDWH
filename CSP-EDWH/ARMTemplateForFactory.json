									},
									"type": "int"
								},
								"pROWS_WRITTEN": {
									"value": {
										"value": "@activity('INCREMENTAL_COPY_ACTIVITY').output.rowsCopied",
										"type": "Expression"
									},
									"type": "int"
								},
								"pCOPY_DURATION": {
									"value": {
										"value": "@activity('INCREMENTAL_COPY_ACTIVITY').output.copyDuration",
										"type": "Expression"
									},
									"type": "int"
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "CSP_EDWH_LINKED_SERVER",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "SET_END_KO",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [
							{
								"activity": "INCREMENTAL_COPY_ACTIVITY",
								"dependencyConditions": [
									"Failed"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
