										"type": "String",
										"value": {
											"value": "@pipeline().parameters.DN_OUT_TABLE_NAME",
											"type": "Expression"
										}
									}
								}
							},
							"dataset": {
								"referenceName": "EDWH_DB",
								"type": "DatasetReference",
								"parameters": {}
							}
						}
