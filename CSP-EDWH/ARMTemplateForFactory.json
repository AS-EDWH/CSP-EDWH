				"parameters": {
					"DN_SOURCE": {
						"type": "string"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/CSP_INCREMENTAL_LOAD')]",
				"[concat(variables('factoryId'), '/pipelines/LOAD_L0_TABLES_OLD')]"
