							"enableStaging": false,
							"parallelCopies": 1
						},
						"inputs": [
							{
								"referenceName": "DS_L0_TABLE_IMPROVE",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "EDWH_LOAD_L0",
								"type": "DatasetReference",
								"parameters": {
									"DN_OUT_TABLE_NAME": {
